{
    "name": "mypy111",
    "dockerFile": "Dockerfile",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance",
                "ms-python.isort",
                "ms-python.black-formatter"
            ],
            "settings": {
                "[python]": {
                    "editor.defaultFormatter": "ms-python.black-formatter", 
                    "editor.formatOnSave": true,
                    "editor.codeActionsOnSave": {
                    "source.organizeImports": "explicit"
                    }
                }
            }
        }
    }
}